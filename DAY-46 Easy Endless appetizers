/* package codechef; // don't place package name! */

import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner hh = new Scanner(System.in);
		int t = hh.nextInt();
		for(int i =0;i<t;i++){
		    int x = hh.nextInt();
		    int y = hh.nextInt();
		    int r = hh.nextInt();
		    int z = r/30;
		    int tot = x+z;
		    if(tot%y==0)
		        System.out.println(tot/y);
		    else
		        System.out.println((tot/y)+1);
		}
	}
}
